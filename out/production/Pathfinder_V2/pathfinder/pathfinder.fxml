<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" prefHeight="800.0" prefWidth="1051.0" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pathfinder.PathfinderCompanion">
   <children>
      <VBox prefHeight="800.0" prefWidth="250.0" HBox.hgrow="NEVER">
         <children>
            <Button fx:id="btnNewBoard" mnemonicParsing="false" prefHeight="45.0" prefWidth="97.0" text="New board">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnRandomObstacles" mnemonicParsing="false" prefHeight="52.0" prefWidth="137.0" text="Random obstacles">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnRun" mnemonicParsing="false" prefHeight="53.0" prefWidth="119.0" text="Find Path">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </VBox>
      <Pane fx:id="pane" prefWidth="800.0" prefHeight="800.0"/>
   </children>
</HBox>
